        "category": "RubyMate",
        "icon": "$(graph)"
      },
      {
        "command": "rubymate.validateConfiguration",
        "title": "Validate Configuration",
        "category": "RubyMate",
        "icon": "$(check)"
      },
      {
        "command": "rubymate.showTelemetry",
        "title": "Show Usage Statistics",
        "category": "RubyMate",
        "icon": "$(graph-line)"
      },
      {
        "command": "rubymate.exportTelemetry",
        "title": "Export Telemetry Data",
        "category": "RubyMate",
        "icon": "$(export)"
      },
      {
        "command": "rubymate.clearTelemetry",
        "title": "Clear Telemetry Data",
        "category": "RubyMate",
        "icon": "$(trash)"
      },
      {
        "command": "rubymate.quickDebug",
        "title": "Quick Debug Current File",
        "category": "RubyMate",
        "icon": "$(debug-start)"
      },
      {
        "command": "rubymate.debugCurrentTest",
        "title": "Debug Current Test",
        "category": "RubyMate",
        "icon": "$(debug-alt)"
      },
      {
        "command": "rubymate.addExceptionBreakpoint",
        "title": "Add Exception Breakpoint",
        "category": "RubyMate",
        "icon": "$(debug-breakpoint-conditional)"
      },
      {
        "command": "rubymate.rails.showCommands",
        "title": "Show Rails Commands",
        "category": "Rails",
        "icon": "$(ruby)"
      },
      {
        "command": "rubymate.rails.navigateToModel",
