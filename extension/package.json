    "snippets": [
      {
        "language": "ruby",
        "path": "./snippets/ruby.json"
      },
      {
        "language": "ruby",
        "path": "./snippets/rails.json"
      }
    ],
    "configuration": {
      "title": "RubyMate",
      "properties": {
        "rubymate.enableRubyLSP": {
          "type": "boolean",
          "default": false,
          "description": "DEPRECATED: Ruby LSP integration removed due to compatibility issues. RubyMate now uses custom indexers and providers."
        },
        "rubymate.enableSolargraph": {
          "type": "boolean",
          "default": true,
          "description": "Enable Solargraph for enhanced completions"
        },
        "rubymate.formatOnSave": {
          "type": "boolean",
          "default": false,
          "description": "Auto-format Ruby files on save using RuboCop"
        },
        "rubymate.rubyPath": {
          "type": "string",
          "default": "ruby",
          "description": "Path to Ruby executable"
        },
        "rubymate.gemPath": {
          "type": "string",
          "default": "",
          "description": "Path to gems directory (optional)"
        },
        "rubymate.enableRailsSupport": {
          "type": "boolean",
          "default": true,
          "description": "Enable Rails-specific features"
        },
        "rubymate.testFramework": {
          "type": "string",
          "enum": [
            "rspec",
            "minitest",
            "auto"
          ],
          "default": "auto",
          "description": "Test framework to use"
        },
        "rubymate.checkRequiredGems": {
          "type": "boolean",
          "default": false,
          "description": "DEPRECATED: No longer needed as ruby-lsp integration was removed"
        },
        "rubymate.enableN1Detection": {
          "type": "boolean",
          "default": true,
          "description": "Enable N+1 query detection for ActiveRecord"
        },
        "rubymate.n1DetectionExcludePaths": {
          "type": "array",
          "default": [],
          "description": "Glob patterns to exclude from N+1 detection (e.g., '**/lib/**', '**/services/**')",
          "items": {
            "type": "string"
          }
        },
        "rubymate.autoInsertEnd": {
          "type": "boolean",
          "default": true,
          "description": "Automatically insert 'end' keyword when creating Ruby blocks (def, class, if, etc.)"
        }
      }
    },
    "debuggers": [
      {
        "type": "ruby",
        "label": "Ruby Debug (rdbg)",
        "languages": [
          "ruby"
        ],
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "Absolute path to the Ruby file to debug",
                "default": "${file}"
              },
              "args": {
                "type": "array",
                "description": "Command line arguments passed to the program",
                "default": []
              },
              "cwd": {
                "type": "string",
                "description": "Working directory of the program being debugged",
                "default": "${workspaceFolder}"
              },
              "env": {
                "type": "object",
                "description": "Environment variables defined as key-value pairs",
                "default": {}
              },
              "useBundler": {
                "type": "boolean",
                "description": "Use bundle exec to run the debugger",
                "default": true
              },
              "stopOnEntry": {
                "type": "boolean",
                "description": "Automatically stop after launch",
                "default": false
              },
              "showDebuggerOutput": {
                "type": "boolean",
                "description": "Show debugger output in the debug console",
                "default": false
              },
              "pathMappings": {
                "type": "object",
                "description": "Path mappings for remote debugging",
                "default": {}
              }
            }
          },
          "attach": {
            "properties": {
              "remoteHost": {
                "type": "string",
                "description": "Remote host to attach to",
                "default": "localhost"
              },
              "remotePort": {
                "type": "number",
                "description": "Remote port to attach to",
                "default": 12345
              },
              "cwd": {
                "type": "string",
                "description": "Working directory",
                "default": "${workspaceFolder}"
              },
              "pathMappings": {
                "type": "object",
                "description": "Path mappings for remote debugging",
                "default": {}
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "ruby",
            "request": "launch",
            "name": "Debug Current File",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "useBundler": true
          },
          {
            "type": "ruby",
            "request": "launch",
            "name": "Debug Rails Server",
            "program": "${workspaceFolder}/bin/rails",
            "args": [
              "server"
            ],
            "cwd": "${workspaceFolder}",
            "useBundler": true,
            "env": {
              "RAILS_ENV": "development"
            }
          },
          {
            "type": "ruby",
            "request": "launch",
            "name": "Run RSpec - Current File",
            "program": "${workspaceFolder}/bin/rspec",
            "args": [
              "${file}"
            ],
            "cwd": "${workspaceFolder}",
            "useBundler": true
          },
          {
            "type": "ruby",
            "request": "attach",
            "name": "Attach to Remote",
            "remoteHost": "localhost",
            "remotePort": 12345
          }
        ],
        "configurationSnippets": [
          {
            "label": "Ruby: Debug Current File",
            "description": "Debug the currently open Ruby file",
            "body": {
              "type": "ruby",
              "request": "launch",
              "name": "Debug Current File",
              "program": "^\"\\${file}\"",
              "cwd": "^\"\\${workspaceFolder}\"",
              "useBundler": true
            }
          },
          {
            "label": "Ruby: Debug Rails Server",
            "description": "Debug Rails server",
            "body": {
              "type": "ruby",
              "request": "launch",
              "name": "Debug Rails Server",
              "program": "^\"\\${workspaceFolder}/bin/rails\"",
              "args": [
                "server"
              ],
              "cwd": "^\"\\${workspaceFolder}\"",
              "useBundler": true
            }
          },
          {
            "label": "Ruby: Debug RSpec Test",
            "description": "Debug RSpec test file",
            "body": {
              "type": "ruby",
              "request": "launch",
              "name": "Debug RSpec Test",
              "program": "^\"\\${workspaceFolder}/bin/rspec\"",
              "args": [
                "^\"\\${file}\""
              ],
              "cwd": "^\"\\${workspaceFolder}\"",
              "useBundler": true
            }
          },
          {
            "label": "Ruby: Attach to Remote Debugger",
            "description": "Attach to a remote Ruby debugger",
            "body": {
              "type": "ruby",
              "request": "attach",
              "name": "Attach to Remote",
              "remoteHost": "localhost",
              "remotePort": 12345
            }
          }
        ]
      }
    ],
    "breakpoints": [
      {
        "language": "ruby"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile:prod",
    "compile": "node esbuild.js",
    "compile:prod": "node esbuild.js --production",
    "watch": "node esbuild.js --watch",
    "pretest": "npm run compile && tsc -p ./",
    "test": "node ./out/test/runTest.js",
    "test:unit": "npm run pretest && node ./out/test/runTest.js",
    "package": "npm run compile:prod && vsce package",
    "publish": "npm run compile:prod && vsce publish",
    "analyze": "npm run compile:prod && echo 'Check bundle analysis above'"
  },
