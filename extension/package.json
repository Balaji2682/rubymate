{
  "name": "rubymate",
  "displayName": "Ruby & Rails - Complete IDE: LSP, Debugger, Test Explorer, Intellisense",
  "description": "Complete Ruby on Rails development: Language Server (LSP), Solargraph, Debugger, Test Explorer, RSpec, Minitest, RuboCop, ActiveRecord intelligence, Code Navigation, Auto-complete",
  "version": "0.2.3",
  "publisher": "BalajiR",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#CC342D",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.85.0",
    "node": ">=20.0.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Snippets",
    "Formatters",
    "Testing",
    "Linters"
  ],
  "keywords": [
    "ruby",
    "rails",
    "ruby on rails",
    "ruby language",
    "ruby ide",
    "rails ide",
    "ruby development",
    "rails development",
    "ruby debugger",
    "rails debugger",
    "ruby lsp",
    "language server",
    "solargraph",
    "ruby autocomplete",
    "ruby intellisense",
    "ruby code completion",
    "rspec",
    "minitest",
    "test explorer",
    "ruby testing",
    "rails testing",
    "ruby test",
    "rubocop",
    "ruby linter",
    "ruby formatter",
    "ruby format",
    "activerecord",
    "ruby navigation",
    "go to definition",
    "find references",
    "ruby refactoring",
    "erb",
    "haml",
    "slim",
    "bundler",
    "gem",
    "rake",
    "ruby snippets",
    "rails snippets",
    "ruby syntax",
    "rails syntax",
    "ruby highlighting",
    "debug",
    "debugging",
    "breakpoints",
    "ruby tools",
    "rails tools"
  ],
  "preview": false,
  "qna": "marketplace",
  "badges": [
    {
      "url": "https://img.shields.io/visual-studio-marketplace/v/BalajiR.rubymate",
      "href": "https://marketplace.visualstudio.com/items?itemName=BalajiR.rubymate",
      "description": "Version"
    },
    {
      "url": "https://img.shields.io/visual-studio-marketplace/i/BalajiR.rubymate",
      "href": "https://marketplace.visualstudio.com/items?itemName=BalajiR.rubymate",
      "description": "Installs"
    }
  ],
  "activationEvents": [
    "onLanguage:ruby",
    "workspaceContains:**/*.rb"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ruby",
        "extensions": [
          ".rb",
          ".rake",
          ".gemspec",
          "Rakefile",
          "Gemfile"
        ],
        "aliases": [
          "Ruby",
          "ruby"
        ]
      }
    ],
    "keybindings": [
      {
        "key": "alt+shift+g",
        "command": "rubymate.goToClass",
        "when": "editorLangId == ruby"
      },
      {
        "key": "alt+shift+r",
        "command": "rubymate.navigateToRelated",
        "when": "editorLangId == ruby"
      },
      {
        "key": "alt+shift+t",
        "command": "rubymate.toggleSpec",
        "when": "editorLangId == ruby"
      },
      {
        "key": "alt+shift+f",
        "command": "rubymate.fileStructure",
        "when": "editorLangId == ruby"
      },
      {
        "key": "alt+shift+s",
        "command": "rubymate.searchEverywhere",
        "when": "editorLangId == ruby"
      }
    ],
    "commands": [
      {
        "command": "rubymate.goToClass",
        "title": "Go to Class",
        "category": "RubyMate",
        "icon": "$(symbol-class)"
      },
      {
        "command": "rubymate.fileStructure",
        "title": "File Structure",
        "category": "RubyMate",
        "icon": "$(list-tree)"
      },
      {
        "command": "rubymate.searchEverywhere",
        "title": "Search Everywhere",
        "category": "RubyMate",
        "icon": "$(search)"
      },
      {
        "command": "rubymate.navigateToRelated",
        "title": "Navigate to Related File",
        "category": "RubyMate",
        "icon": "$(link)"
      },
      {
        "command": "rubymate.toggleSpec",
        "title": "Toggle Between Code and Spec",
        "category": "RubyMate",
        "icon": "$(arrow-swap)"
      },
      {
        "command": "rubymate.runTest",
        "title": "Run Test at Cursor",
        "category": "RubyMate",
        "icon": "$(debug-alt-small)"
      },
      {
        "command": "rubymate.runTestFile",
        "title": "Run Test File",
        "category": "RubyMate",
        "icon": "$(run-all)"
      },
      {
        "command": "rubymate.startDebugger",
        "title": "Start Debugger",
        "category": "RubyMate",
        "icon": "$(debug)"
      },
      {
        "command": "rubymate.reindexWorkspace",
