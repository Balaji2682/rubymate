      {
        "command": "rubymate.showCallHierarchy",
        "title": "Show Call Hierarchy",
        "category": "Navigation",
        "icon": "$(call-hierarchy)"
      },
      {
        "command": "rubymate.showTypeHierarchy",
        "title": "Show Type Hierarchy",
        "category": "Navigation",
